# Configurações do Servidor
PORT=3000
NODE_ENV=development

# Configurações do Banco de Dados
DB_HOST=postgres
DB_PORT=5432
DB_USERNAME=netprovider
DB_PASSWORD=your_password_here
DB_DATABASE=netprovider
DB_SYNCHRONIZE=true
DB_LOGGING=true
DB_SSL=false

# Configurações JWT
JWT_SECRET=your_jwt_secret_here
JWT_EXPIRATION=1d
JWT_REFRESH_EXPIRATION=7d

# Redis
REDIS_HOST=redis
REDIS_PORT=6379

# Kafka
KAFKA_BROKERS=kafka:9092

# CORS
CORS_ORIGIN=* 